{
  "env": {
    "browser": true,
    "commonjs": false,
    "node": false
  },
  "plugins": [
  ],
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module",
    "ecmaVersion": 2018
  },
  "rules": {
    "react/prop-types": 0,
    "no-extra-semi": 1,
    "no-unexpected-multiline": 1
  }
}